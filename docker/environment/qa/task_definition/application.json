{
    "family": "Application",
    "taskRoleArn": "$Role_ARN",
    "executionRoleArn": "$Role_ARN",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "application",
            "image": "",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "application-3000-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [],
            "environment": [],
            "environmentFiles": [
                {
                    "value": "arn:aws:s3:::$ENV_BUCKET/application.env",
                    "type": "s3"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "public",
                    "containerPath": "/share/public",
                    "readOnly": false
                },
                {
                    "sourceVolume": "storage",
                    "containerPath": "/srv/primero/application/storage",
                    "readOnly": false
                }
            ],
            "volumesFrom": [],
            "secrets": [],
            "dependsOn": []
        }
    ],
    "volumes": [
        {
            "name": "public",
            "efsVolumeConfiguration": {
                "fileSystemId": "$FS_ID",
                "rootDirectory": "public"
            }
        },
        {
            "name": "storage",
            "efsVolumeConfiguration": {
                "fileSystemId": "$FS_ID",
                "rootDirectory": "storage"
            }
        }
    ],
    "placementConstraints": [],
    "compatibilities": [
        "EC2",
        "FARGATE"
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": [
        {
            "key": "service",
            "value": "application"
        }
    ]
}
